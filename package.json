{
  "name": "nest_crud",
  "version": "0.0.1",
  "description": "A simple CRUD (Create, Read, Update, Delete) application built with NestJS and TypeORM.",
  "author": "PanasheT",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "preimporter": "nest build importer",
    "importer": "node dist/apps/importer/main.js import",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run": "npm run typeorm migration:run -- -d ./typeorm.config.ts",
    "typeorm:revert": "npm run typeorm -- -d ./typeorm.config.ts migration:revert",
    "root": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./root_script.ps1",
    "directory": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./directory_script.ps1"
  },
  "dependencies": {
    "@casl/ability": "^6.3.1",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/throttler": "^3.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@sendgrid/mail": "^7.7.0",
    "@types/jsonwebtoken": "^8.5.9",
    "bcrypt": "^5.0.1",
    "cache-manager": "^4.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.18.1",
    "express-basic-auth": "^1.2.1",
    "fs": "^0.0.1-security",
    "gravatar": "^1.8.2",
    "handlebars": "^4.7.7",
    "hbs": "^4.2.0",
    "helmet": "^5.1.1",
    "html-pdf-node": "^1.0.8",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "luxon": "^3.1.0",
    "module-alias": "^2.2.2",
    "nest-commander": "^3.0.0",
    "nestjs-typeorm-paginate": "^4.0.3",
    "nodemailer": "^6.7.8",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^8.7.3",
    "puppeteer": "^19.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.7",
    "typeorm-naming-strategies": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.14",
    "@types/gravatar": "^1.8.3",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.182",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.5",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
